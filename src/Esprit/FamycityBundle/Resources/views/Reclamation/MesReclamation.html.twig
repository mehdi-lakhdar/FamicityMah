{% extends '@EspritFamycity/secondlayout.html.twig' %}

{% block content %}

    <!-- Content Wrapper. Contains page content -->
    <div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <section class="content-header">
        <h1>
            Reclamation

        </h1>
        <ol class="breadcrumb">
            <li><a href="{{path("esprit_famycity_homepage") }}"><i class="fa fa-dashboard"></i> Home</a></li>
            <li class="active">Reclamation</li>
        </ol>
    </section>

    <!-- Main content -->
    <section class="content">

    <!-- row -->
    <div class="row">
    <div class="col-md-12">
        <!-- The time line -->
        <ul class="timeline">
            <!-- timeline time label -->

            <!-- /.timeline-label -->
            <!-- timeline item -->
            <li>


                <div class="timeline-item">


                    <h3 class="timeline-header"><a href="{{path("Reclamation") }}">Cr√©er une reclamation</a></h3>

                    <div class="timeline-body">
                        <table border="1" class="text-center" align="center" style="width: 100%">

                            <tr class="bg-green-active">
                                <td> Titre</td>
                                <td> objet </td>
                                <td>Description</td>
                                <td></td>
                                <td></td>
                                <td>Etat</td>
                            </tr>

                            {% for i in m %}
                                <tr>
                                    <td>{{ i.titre }}</td>
                                    <td>{{ i.description }}</td>
                                    <td>{{ i.objet }}</td>

                                    <td><a href="{{ path('delete_Reclmation',{'id':i.id})}}" class="btn btn-danger btn-xs">Supprimer</a> </td>
                                    <td><a href="{{ path('update_Reclamation',{'id':i.id})}}"  class="btn btn-primary btn-xs">Modifier</a> </td>

                                    <td>Non trai</td>
                                </tr>
                            {% endfor %}
                        </table>
                    </div>


                    <!-- /.box-body -->
                </div>
                <!-- /.box -->

                <!-- /.col -->


                <div class="control-sidebar-bg"></div>
            </li>


        </ul>
    </div>
    <!-- ./wrapper -->

    <!-- jQuery 2.2.3 -->
